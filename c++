#include <stdio.h>

int main()
{
   
    int x, y, z, med;
   
    printf("digite os 3 valores:");
    scanf("%d""%d""%d", &x, &y, &z);
    med=(x+y+z)/3;
    if(x>=7)
    {
        printf("Aprovado com média %d",med);
    }
    else
    {
        printf("Reprovado com média %d",med);

    }
    return 0;
    return 0;
}
